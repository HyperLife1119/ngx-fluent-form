{
  "name": "ngx-fluent-form",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm run build:lib && npm run build:schematics",
    "build:docs": "docgeni build --base-href=/fluent-form/",
    "build:lib": "ng build ngx-fluent-form --configuration production",
    "build:schematics": "cd projects/ngx-fluent-form && npm run build:schematics",
    "coverage": "ng test ngx-fluent-form --no-watch --code-coverage --browsers=ChromeHeadless",
    "coverage:dev": "ng test ngx-fluent-form --code-coverage",
    "dev": "ng build ngx-fluent-form --watch",
    "dev:docs": "docgeni serve --port 4600",
    "lint": "ng lint",
    "postbuild": "copyfiles ./README.md ./README.zh-CN.md ./LICENSE ./dist/ngx-fluent-form/",
    "prepare": "husky install",
    "publish": "npm publish ./dist/ngx-fluent-form",
    "test": "ng test ngx-fluent-form --configuration production",
    "test:dev": "ng test ngx-fluent-form"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@angular/animations": "~17.3.0",
    "@angular/cdk": "~17.3.0",
    "@angular/common": "~17.3.0",
    "@angular/compiler": "~17.3.0",
    "@angular/core": "~17.3.0",
    "@angular/forms": "~17.3.0",
    "@angular/platform-browser": "~17.3.0",
    "@angular/platform-browser-dynamic": "~17.3.0",
    "@angular/platform-server": "~17.3.0",
    "@angular/router": "~17.3.0",
    "@angular/ssr": "~17.3.0",
    "@ng-doc/app": "^17.6.10",
    "@ng-doc/builder": "^17.6.10",
    "@ng-doc/core": "^17.6.10",
    "@ng-doc/ui-kit": "^17.6.10",
    "express": "~4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.3.0",
    "@angular-devkit/core": "~17.3.0",
    "@angular-devkit/schematics": "~17.3.0",
    "@angular-eslint/builder": "~17.3.0",
    "@angular-eslint/eslint-plugin": "~17.3.0",
    "@angular-eslint/eslint-plugin-template": "~17.3.0",
    "@angular-eslint/schematics": "~17.3.0",
    "@angular-eslint/template-parser": "~17.3.0",
    "@angular/cli": "~17.3.0",
    "@angular/compiler-cli": "~17.3.0",
    "@angular/language-service": "~17.3.0",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@ngify/types": "^1.5.1",
    "@nx/angular": "19.0.8",
    "@nx/eslint": "19.0.8",
    "@nx/eslint-plugin": "19.0.8",
    "@nx/jest": "19.0.8",
    "@nx/js": "19.0.8",
    "@schematics/angular": "~17.3.0",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@types/express": "4.17.14",
    "@types/jest": "^29.4.0",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^9.0.2",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~14.0.3",
    "lint-staged": "^15.0.0",
    "ng-packagr": "~17.3.0",
    "nx": "19.0.8",
    "postcss": "^8.4.5",
    "postcss-url": "~10.1.3",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.4.1"
  }
