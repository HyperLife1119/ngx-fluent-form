{
  "name": "ngx-fluent-form",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng build ngx-fluent-form --watch",
    "build": "npm run build:lib && npm run build:schematics",
    "build:lib": "ng build ngx-fluent-form --configuration production",
    "build:schematics": "cd projects/ngx-fluent-form && npm run build:schematics",
    "postbuild": "copyfiles ./README.md ./README.zh-CN.md ./LICENSE ./dist/ngx-fluent-form/",
    "test:watch": "ng test ngx-fluent-form",
    "test": "ng test ngx-fluent-form --configuration production",
    "coverage:watch": "ng test ngx-fluent-form --code-coverage",
    "coverage": "ng test ngx-fluent-form --no-watch --code-coverage --browsers=ChromeHeadless",
    "lint": "ng lint",
    "publish": "npm publish ./dist/ngx-fluent-form",
    "docs:json": "compodoc --config=./compodoc.json",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.9",
    "@angular/common": "15.2.9",
    "@angular/compiler": "15.2.9",
    "@angular/core": "15.2.9",
    "@angular/forms": "15.2.9",
    "@angular/platform-browser": "15.2.9",
    "@angular/platform-browser-dynamic": "15.2.9",
    "@angular/router": "15.2.9",
    "ng-zorro-antd": "15.1.0",
    "rxjs": "7.8.0",
    "tslib": "2.5.0",
    "zone.js": "0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.9",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "15.2.9",
    "@angular/compiler-cli": "15.2.9",
    "@babel/core": "7.21.0",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@compodoc/compodoc": "1.1.19",
    "@ngify/types": "1.5.1",
    "@storybook/addon-actions": "6.5.14",
    "@storybook/addon-essentials": "6.5.14",
    "@storybook/addon-interactions": "6.5.14",
    "@storybook/addon-links": "6.5.14",
    "@storybook/addon-storysource": "6.5.14",
    "@storybook/angular": "6.5.14",
    "@storybook/builder-webpack5": "6.5.14",
    "@storybook/manager-webpack5": "6.5.14",
    "@storybook/mdx2-csf": "0.0.3",
    "@storybook/testing-library": "0.0.13",
    "@types/jasmine": "4.3.1",
    "@types/node": "16.18.25",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "babel-loader": "9.1.0",
    "copyfiles": "2.4.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jasmine-core": "4.6.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lint-staged": "^13.1.0",
    "markdown-to-jsx": "7.1.7",
    "ng-packagr": "15.2.2",
